<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
	prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.FxmlController">
	<children>
		<VBox layoutX="31.0" layoutY="47.0" prefHeight="200.0" prefWidth="100.0"
			spacing="4.0" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0"
			AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="8.0">
			<children>
				<HBox fillHeight="false">
					<children>
						<LineChart fx:id="areaBattery" maxHeight="50.0"
							prefHeight="50.0" title="Battery">
							<xAxis>
								<CategoryAxis side="BOTTOM" />
							</xAxis>
							<yAxis>
								<NumberAxis side="LEFT" />
							</yAxis>
						</LineChart>
						<LineChart fx:id="areaCycleTime" maxHeight="50.0"
							prefHeight="50.0" title="Cycle Time">
							<xAxis>
								<CategoryAxis side="BOTTOM" />
							</xAxis>
							<yAxis>
								<NumberAxis side="LEFT" />
							</yAxis>
						</LineChart>
					</children>
				</HBox>
				<HBox fillHeight="false" prefHeight="155.0" prefWidth="784.0">
					<children>
						<LineChart fx:id="areaGyroAngel" maxHeight="50.0"
							prefHeight="50.0" title="Gyro Angel">
							<xAxis>
								<CategoryAxis side="BOTTOM" />
							</xAxis>
							<yAxis>
								<NumberAxis side="LEFT" />
							</yAxis>
						</LineChart>
						<LineChart fx:id="areaOffset" maxHeight="50.0"
							prefHeight="50.0" title="Offset">
							<xAxis>
								<CategoryAxis side="BOTTOM" />
							</xAxis>
							<yAxis>
								<NumberAxis side="LEFT" />
							</yAxis>
						</LineChart>
					</children>
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</HBox>
				<LineChart fx:id="areaMotor" maxHeight="50.0" prefHeight="50.0"
					title="Motor">
					<xAxis>
						<CategoryAxis side="BOTTOM" />
					</xAxis>
					<yAxis>
						<NumberAxis side="LEFT" />
					</yAxis>
				</LineChart>
				<HBox spacing="16.0">
					<children>
						<VBox spacing="16.0">
							<children>
								<HBox spacing="16.0">
									<children>
										<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Kp:" />
										<TextField fx:id="tfKp" />
									</children>
								</HBox>
								<HBox spacing="16.0">
									<children>
										<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tn:" />
										<TextField fx:id="tfTn" />
									</children>
								</HBox>
								<HBox spacing="16.0">
									<children>
										<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tv:" />
										<TextField fx:id="tfTv" />
									</children>
								</HBox>
								<Button fx:id="btSend" mnemonicParsing="false"
									onAction="#buttonSendOnAction" text="Senden" />
							</children>
						</VBox>
						<VBox>
							<children>
								<HBox>
									<children>
										<Button fx:id="btUp" mnemonicParsing="false" onAction="#buttonUp"
											text="up" />
									</children>
								</HBox>
								<HBox>
									<children>
										<Button fx:id="btLeft" mnemonicParsing="false"
											onAction="#buttonLeft" text="&lt;" />
										<Button fx:id="btDown" mnemonicParsing="false"
											onAction="#buttonDown" text="down" />
										<Button fx:id="btRight" mnemonicParsing="false"
											onAction="#buttonRight" text="&gt;" />
									</children>
								</HBox>
							</children>
						</VBox>
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
